{
  "name": "travel2x-storage",
  "version": "1.0.0",
  "description": "travel2x storage service",
  "main": "src/server.ts",
  "scripts": {
    "dev": "tsx watch ./src/server.ts | pino-pretty",
    "build": "tsc && tsc-alias",
    "start": "NODE_ENV=production node dist/server.js",
    "migration:run": "tsx ./src/scripts/migrate-call.ts",
    "prettier:check": "prettier -c src/**",
    "format": "prettier -c --write src/**",
    "eslint:check": "eslint 'src/**'"
  },
  "author": "Travel2X",
  "license": "ISC",
  "engines": {
    "node": ">= 18.0.0"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.476.0",
    "@aws-sdk/lib-storage": "^3.476.0",
    "@aws-sdk/node-http-handler": "^3.374.0",
    "@aws-sdk/s3-request-presigner": "^3.476.0",
    "@fastify/multipart": "^8.0.0",
    "@fastify/rate-limit": "^9.0.1",
    "@isaacs/ttlcache": "^1.4.1",
    "async-retry": "^1.3.3",
    "axios": "^1.6.2",
    "axios-retry": "^4.0.0",
    "crypto-js": "^4.2.0",
    "dotenv": "^16.3.1",
    "fastify": "^4.25.1",
    "fastify-cors": "^6.1.0",
    "fastify-plugin": "^4.5.1",
    "ioredis": "^5.3.2",
    "json-schema-to-ts": "^2.12.0",
    "jsonwebtoken": "^9.0.2",
    "knex": "^3.1.0",
    "md5-file": "^5.0.0",
    "pg": "^8.11.3",
    "pg-boss": "^9.0.3",
    "pg-listen": "^1.7.0",
    "pino": "^8.17.1",
    "pino-logflare": "^0.4.2",
    "postgres-migrations": "^5.3.0"
  },
  "devDependencies": {
    "@types/async-retry": "^1.4.8",
    "@types/crypto-js": "^4.2.1",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.10.5",
    "@types/pg": "^8.10.9",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.0",
    "pino-pretty": "^10.3.0",
    "prettier": "^3.1.1",
    "ts-node-dev": "^2.0.0",
    "tsc-alias": "^1.8.8",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.6.2",
    "typescript": "^5.3.3"
  }
}
